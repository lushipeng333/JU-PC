.el-input__inner{
    width: 100%;
  }
  .el-form--inline .el-form-item__label {
    display: block;
    text-align: left;
  }
  .el-form-item {
    margin: 0;
    padding-left: 5px;
    padding-right: 5px;
  }
.el-form{
  width: 100%;
  height: 100%;
}

  .el-form-item__label{
   padding-right: 0px;
   /* padding-top: 10px; */
  }
  .el-form,.el-form--inline{
    padding-left: 5px;
    padding-right: 5px;
  }
 .el-row{
    margin-top: 20px;
  }
  .pagination-container {
    position: fixed;
    right: 100px;
    bottom: 12px;

  }
  .pagination-container .el-pagination {
    right: 0;
    bottom: 0px;
    position: absolute;
    background-color: #FFFFFF;
    z-index: 1000;
  }
  .el-select__tags{
    overflow: hidden;
    flex-wrap: nowrap;
  }
  .input1 .el-input__inner {
    height: 20px;
    padding-left: 5px;

  }
  .el-tabs__content{
    margin-top: 20px;
    overflow-x: auto;
    height: 100%;
  }

  .el-form--inline .el-form-item__label{
    display: flex;
  }

  .el-table__body-wrapper::-webkit-scrollbar-track-piece {
  /* //滚动条凹槽的颜色，还可以设置边框属性 */
  background-color: #dcdfe3;
}
  .el-table__body-wrapper::-webkit-scrollbar-thumb {
  /* //滚动条的设置 */
  background-color: #c9cacd;
  /* background-image: linear-gradient(#ffffff , #c9cacd); */
  background-clip: padding-box;
  min-height: 28px;
  border: 1px solid #b0b2b6;
  border-radius: 8px;
}
/* /deep/ ::-webkit-scrollbar-thumb:hover {
  background-image: linear-gradient(#bddeff , #a2d2ff);
  border: 1px solid #84b8ef;
} */
  /* // 滚动条的宽度 */
  .el-table__body-wrapper::-webkit-scrollbar {
    width: 16px;
    /* // 横向滚动条 */
    height: 16px;
    /* // 纵向滚动条 必写 */
  }
.table-cell-class .el-input--medium{
    position: relative;
  }
  .table-cell-class .el-form-item__error{
    display: inline-block;
    position: absolute;
    top: 10px;
    right: 10px;
    font-size: 20px;
  }
  .table-cell-class .el-input__inner{
    border-radius: 0;
    border: none;
  }
  .table-cell-class .cell{
    padding: 0px !important;
  }
  .table-cell-class .cell .el-form-item {
    padding: 0px;
    width: 100%;
    margin: 0;
  }
  .table-cell-class .cell .el-form-item .el-form-item__content{
     width: 100%;
  }
.table-cell-class .cell .el-form-item .el-form-item__content .el-select{
     width: 100%;
  }
.el-popover{
    /* text-align: center; */
  }
  /* 用来设置当前页面element全局table的内间距 */

  /* 用来设置当前页面element全局table 选中某行时的背景色*/
  .el-table__body tr.current-row>td {
    background-color: #a5d9f1 !important;
    /* color: #f19944; */
    /* 设置文字颜色，可以选择不设置 */
  }

  /* 用来设置当前页面element全局table 鼠标移入某行时的背景色*/
  .el-table--enable-row-hover .el-table__body tr:hover>td {
    background-color: #a5d9f1;
    /* color: #f19944; */
    /* 设置文字颜色，可以选择不设置 */
  }
.ivu-tabs-content {
    height: 93%;
  }

 .ivu-tabs-bar {
    margin: 0;
